var year= 2020;

if(year % 4 === 0)
{ 
    if(year % 100 === 0)
    {
        if(year % 400 === 0)
        {
            console.log("Its a leap year");
        }
        else
        {
            console.log("Not a leap year")
        }

       
    }
    else
    {
       console.log("Its a leap year");   
    }
   
     console.log("Its a leap year");

}
else
{
    console.log("Not a Leap Year");
}
